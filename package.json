  "platform": "test",
  "build": {
    "productName": "heqi",
    "appId": "Mac Developer: 55377146@qq.com (8S22CQ8UK8)",
    "asar": true,
    "readonly": false,
    "publish": [
      {
        "provider": "generic",
        "url": "http://heqiset.heqiauto.com",
        "useMultipleRangeRequest": false
      }
